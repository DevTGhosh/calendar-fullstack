{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "plugin:import/errors", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  },
  "plugins": ["promise", "import"],
  "globals": {
    "process": true
  }
}
